<?xml version="1.0"?>
<strings>
    <!-- System ISOD -->
    <ISOD>
        <!-- Powiadomienia -->
        <notifications>
            <string key="hello_isod_notification_title">Konto ISOD pomyślnie połączone!</string>
            <string key="hello_isod_notification_body">Będziesz otrzymywać powiadomienia z wiadomościami ISOD</string>
            <string key="new_isod_notification_title">Nowe powiadomienie ISOD</string>
        </notifications>

        <!-- Informacje -->
        <info>
            <string key="isod_account_successfully_unlinked_info">Konto ISOD pomyślnie odłączone</string>
            <string key="isod_api_key_expired">Klucz API ISOD wygasł</string>
            <string key="no_isod_account_linked_info">Użytkownik nie ma połączonego konta ISOD</string>
        </info>

        <!-- Błędy -->
        <errors>
            <string key="isod_server_error">Błąd serwera ISOD</string>
            <string key="invalid_isod_auth_data_error">Nieprawidłowa nazwa użytkownika lub klucz API</string>
            <string key="user_has_no_usos_id_in_isod">Użytkownik nie ma identyfikatora USOS w ISOD</string>
        </errors>
    </ISOD>

    <!-- System USOS -->
    <USOS>
        <!-- Powiadomienia -->
        <notifications>
            <string key="hello_usos_notification_title">Konto USOS pomyślnie połączone!</string>
            <string key="hello_usos_notification_body">Będziesz otrzymywać powiadomienia z wiadomościami USOS</string>
            <string key="new_usos_notification_title">Nowe powiadomienie USOS</string>
        </notifications>

        <!-- Informacje -->
        <info>
            <string key="usos_account_successfully_unlinked_info">Konto USOS pomyślnie odłączone</string>
            <string key="usos_session_expired">Sesja USOS wygasła</string>
            <string key="no_usos_account_linked_info">Użytkownik nie ma połączonego konta USOS</string>
        </info>

        <!-- Błędy -->
        <errors>
            <string key="usos_server_error">Błąd serwera USOS</string>
            <string key="usos_session_expired_error">Sesja USOS wygasła</string>
            <string key="invalid_usos_auth_data_error">Podany kod PIN jest nieprawidłowy lub wygasł</string>
        </errors>
    </USOS>

    <!-- Globalne Wiadomości -->
    <Global>
        <!-- Informacje -->
        <info>
            <string key="all_user_data_successfully_removed_info">Dane użytkownika pomyślnie usunięte</string>
            <string key="user_successfully_logged_out_from_all_other_devices">Użytkownik pomyślnie wylogowany ze wszystkich innych urządzeń</string>
            <string key="user_not_found_info">Użytkownik nie znaleziony</string>
        </info>

        <!-- Błędy -->
        <errors>
            <string key="internal_server_error">Wewnętrzny błąd serwera</string>
            <string key="too_many_requests_error">Zbyt wiele żądań</string>
            <string key="invalid_input_data_error">Podano nieprawidłowe dane</string>
            <string key="invalid_fcm_token_error">Podano nieprawidłowy token FCM</string>
            <string key="invalid_data_received_form_external_service">Otrzymano nieprawidłowe dane z zewnętrznej usługi</string>
        </errors>
    </Global>
</strings>
